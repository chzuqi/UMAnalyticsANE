<extension xmlns="http://ns.adobe.com/air/extension/13.0">
	<id>com.czqsoft.umenganalytics</id>
	<versionNumber>1</versionNumber>
	<platforms>

		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libUMeng.a</nativeLibrary>
				<initializer>UMengExtensionInitializer</initializer> 
				<finalizer>UMengExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libUMeng.a</nativeLibrary>
				<initializer>UMengExtensionInitializer</initializer> 
				<finalizer>UMengExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="Android-ARM"> 
			<applicationDeployment> 
				<nativeLibrary>libUMeng.jar</nativeLibrary> 
				<initializer>com.czqsoft.anes.UMAnalyticsExtension</initializer>
				<finalizer>com.czqsoft.anes.UMAnalyticsExtension</finalizer>
			</applicationDeployment> 
		</platform> 

		<platform name="default"> 
			<applicationDeployment/> 
		</platform> 
	</platforms>
</extension>
